<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>KisanLogin</servlet-name>
        <servlet-class>KisanLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PatwariLogin</servlet-name>
        <servlet-class>PatwariLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>KisanDetail</servlet-name>
        <servlet-class>KisanDetail</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>KisanRequests</servlet-name>
        <servlet-class>KisanRequests</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>KisanIdG</servlet-name>
        <servlet-class>KisanIdG</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>KisanLogin</servlet-name>
        <url-pattern>/KisanLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PatwariLogin</servlet-name>
        <url-pattern>/PatwariLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>KisanDetail</servlet-name>
        <url-pattern>/KisanDetail</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>KisanRequests</servlet-name>
        <url-pattern>/KisanRequests</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>KisanIdG</servlet-name>
        <url-pattern>/KisanIdG</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
